    "nette/safe-stream": "^2.3",
    "nette/security": "^2.4",
    "nette/utils": "^2.4",
    "latte/latte": "^2.4",
    "tracy/tracy": "^2.4",
    "fluent/logger": "^1.0",
    "nextras/forms": "^2.0",
    "kdyby/doctrine": "^3.2",
    "kdyby/translation": "^2.5",
    "kdyby/events": "^3.1",
    "ublaboo/datagrid": "^5.4"
